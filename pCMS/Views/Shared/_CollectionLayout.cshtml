    <script type="text/javascript">

        jQuery(document).ready(function () {

            if (!device.mobile() && !device.tablet()) {

                jQuery('header .nav-wrap').tmStickUp({

                    correctionSelector: jQuery('#wpadminbar'), listenSelector: jQuery('.listenSelector'), active: true, pseudo: true
                });

            }
            $(".crop img").one('load', function () {
                $(this).closest('.crop').css({ 'position': 'relative'});
                var pw = $(this).closest('.crop').width();
                var ph = $(this).closest('.crop').height();
                var pratio = pw / ph;
                var imgratio = $(this).width() / $(this).height();
               
                var newwidth = 0;
                var newheight = 0;
                if (pratio > imgratio) {
                    newwidth = pw;
                    newheight = pw / imgratio;

                } else {
                    newwidth = ph * imgratio;
                    newheight = ph;

                }

                $(this).css({
                    'position': 'absolute',
                    'left': '50%',
                    'top': '50%',
                    'margin-left': -newwidth / 2,
                    'margin-top': -newheight / 2,
                    'min-width': newwidth,
                    'width': newwidth,
                    'height': newheight,
                    'min-height': newheight
                });
                if ($(this).closest('.shadow').length > 0) {
                    $(this).closest('.shadow').prepend("<div class='overlayshadow'></div>");
                }
            }).each(function () {
                if (this.complete) $(this).load();
            });
            jQuery('ul.sf-menu').superfish({
                delay: 1000, // the delay in milliseconds that the mouse can remain outside a sub-menu without it closing
                animation: {
                    opacity: "show",
                    height: "show"
                }, // used to animate the sub-menu open
                speed: "normal", // animation speed
                autoArrows: false, // generation of arrow mark-up (for submenu)
                disableHI: true // to disable hoverIntent detection
            });

        })

    </script>

